{
  "name": "bepsvpt/laravel-top-packages",
  "description": "Top 1,000 downloads laravel packages.",
  "license": "AGPL-3.0-only",
  "type": "project",
  "keywords": [
    "laravel",
    "packages"
  ],
  "authors": [
    {
      "name": "bepsvpt",
      "email": "og7lsrszah6y3lz@infinitefa.email"
    }
  ],
  "require": {
    "php": "^8.0",
    "ext-json": "*",
    "ext-pdo": "*",
    "bepsvpt/secure-headers": "~7.2.0",
    "composer/semver": "~3.3.2",
    "doctrine/dbal": "~3.3.7",
    "guzzlehttp/guzzle": "~7.4.5",
    "htmlmin/htmlmin": "~9.0.1",
    "laravel/framework": "~9.18.0",
    "laravel/tinker": "~2.7.2",
    "predis/predis": "~2.0.0",
    "webmozart/assert": "~1.11.0"
  },
  "require-dev": {
    "barryvdh/laravel-ide-helper": "~2.12.3",
    "ergebnis/composer-normalize": "~2.28.0",
    "fakerphp/faker": "~1.19.0",
    "friendsofphp/php-cs-fixer": "~3.8.0",
    "mockery/mockery": "~1.5.0",
    "nunomaduro/collision": "~6.2.0",
    "nunomaduro/larastan": "~2.1.11",
    "phpstan/phpstan-phpunit": "~1.1.1",
    "phpstan/phpstan-webmozart-assert": "~1.2.0",
    "phpunit/phpunit": "~9.5.21",
    "spatie/laravel-ignition": "~1.3.1"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "ergebnis/composer-normalize": true
    },
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --quiet"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate"
    ],
    "update-project": [
      "@php artisan --ansi down",
      "@php artisan --ansi optimize:clear",
      "git pull --rebase --autostash",
      "@composer install --ansi --no-dev -o",
      "@php artisan --ansi optimize",
      "@php artisan --ansi up"
    ]
  }
}
